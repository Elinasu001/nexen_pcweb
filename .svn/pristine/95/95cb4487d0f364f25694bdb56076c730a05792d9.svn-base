.event-popup{
	position: fixed;
	background-color: rgba(0,0,0,0.5);
	top:0;
	width:100%;
	height:100%;
	display: none;
	z-index: 3000;
	.popup-wrap{
		display: grid;
		display: grid;
		justify-content: center;
		align-content: center;
		height:100%;
		width:100%;
	}
	.popup-box{
		background-color: #FFF;
		width:480px;
		
		margin:0 auto;
		.popup-body{

			.slick-arrow{
				position:absolute;
				top: 50%;
				transform: translateY(-50%);
				z-index: 1;
				background-size:100%;
				width: 32px;
				height: 32px;
				outline:none;
				&.slick-prev{
					left: 20px;
					background-image:url( /img/main/event_popup/prev.png);
					content: " ";
					color: transparent;
				}
				&.slick-next{
					background-image:url( /img/main/event_popup/next.png);
					content: " ";
					color: transparent;
					right: 20px;
				}
			}
		}
		.popup-tail{
			padding:20px;
			display: flex;
			flex-wrap: wrap;
			justify-content:space-between;
	
		}

	}
}



