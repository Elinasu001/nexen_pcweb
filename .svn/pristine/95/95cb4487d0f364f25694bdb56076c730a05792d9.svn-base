	.find-size{
		position: absolute;
		bottom: 100px;
		height: 150px;
		width: 100%;
	}
	.find-size-warp{
		display: table;
		margin: 0 auto;
		border-spacing: 20px;
		background-color: #FFF;;
		.car-num-form{
			display: flex;
			align-content:center;
			.img-wrap{
				display: flex;
				align-items:center;
				// justify-content:space-around;
				// display: inline-block;
				// vertical-align: middle;
			}
			.input-wrap{
				margin-left: 15px;
				vertical-align: middle;
				display: inline-block;
				bordeR:1px solid #ccc;
				border-radius: 100px;
				padding:5px;
			}
			input{
				padding:0px 10px;
				height:25px;
				line-height: 25px;;
				width:280px;
				font-size: 14px;
				border:none;
				background: transparent;
				color:#666;
				&::placeholder{font-size: 13px; color:#999; }
				&::-webkit-input-placeholder{font-size: 13px; color:#999; }
				&:-ms-input-placeholder{font-size: 13px; color:#999; }
				&:-mos-input-placeholder {font-size: 13px; color:#999; }
			}
		}
		 img{
			max-width: 100%;
		}
	
		
	
		& > div{
			padding:0 20px;
			display: table-cell;
			position: relative;
			vertical-align:middle;
			min-height: 20px;;
			&::after{
				position: absolute;
				right:-10px;
				top:0;
				content: "";
				border-left:1px solid #ccc;
				height: 100%;;
			}
			&:nth-child(1)::after{
				right: -20px;;
			}
			&:nth-child(2)::after{
				right: 0;
			}
			&:last-child::after{
				display: none;
			}
		}	
	}
	.slick-arrow{
		position:absolute;
		top: 50%;
		transform: translateY(-50%);
		z-index: 1;
		background-size:100%;
		width: 56px;
		height: 56px;
		outline:none;
		&.slick-prev{
			left: 100px;
			background-image:url( /img/main/m_prev.png);
			content: " ";
			color: transparent;
		}
		&.slick-next{
			background-image:url( /img/main/m_next.png);
			content: " ";
			color: transparent;
			right: 100px;
		}
	}
}

