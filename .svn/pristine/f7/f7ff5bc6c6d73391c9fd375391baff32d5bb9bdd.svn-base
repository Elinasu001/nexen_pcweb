a:hover, a:focus { text-decoration: none; color: inherit; }
a:focus { outline: none; }
p { margin: 0; }
.nopg { padding: 0 !important; }
.mg-auto { margin: 0 auto; }

.max500 { max-width: 500px; margin: 0 auto; }
.max600 { max-width: 600px; margin: 0 auto; }
