<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nexwrms.pcweb.com.dao.TitleDao">

	<!-- 언어별 타이틀 조회 -->
	<select id="selectAllList" parameterType="String" resultType="com.nexwrms.pcweb.com.vo.TitleVo">
		/* com.nexwrms.pcweb.com.dao.Title.selectAllList */
		SELECT 
				  LABL_KEY 	AS txtCd
				, LABL 		AS txtContent 
		  FROM 	TB_CM_MOB_LABEL 
		 WHERE 	USE_YN = 'Y'
		   AND	LANG_CD = #{usrLocale}	
		 
	</select>
	
	
</mapper>
