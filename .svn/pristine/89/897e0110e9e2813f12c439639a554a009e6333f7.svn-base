//비회원 주문조회
#modal-nonCustOrd,#modal-affiliatecode{
    width:400px;
    padding:0;
    height: auto;
    transform: translateX(-50%) translateY(-50.1%);
    .modal-inner{
        padding: 30px 30px 20px;
    }
    header.noLine{
        position: absolute;
        top:0;
        left:0;
        width:100%;

        a{
            position: inherit;
            top:15px;
            right:18px;
            width:18px;
            height:18px;
            background-image: url( ../img/common/close_shape.png );
            background-position: 0 0;;
        }
    }
    .popContents{
        
    }
    .input-item{
        padding: 10px 0;;
        input{
            line-height: 40px;
            // font-size: 15px;
            padding:0 10px;
            height:auto;
            width:100%;
            color:#444;

                &::placeholder{font-size: 14px; color:#999; font-weight:normal;}
                &::-webkit-input-placeholder{font-size: 14px; color:#999; font-weight:normal;}
                &:-ms-input-placeholder{font-size: 14px; color:#999; font-weight:normal;}
                &:-mos-input-placeholder {font-size: 14px; color:#999; font-weight:normal;}
        }
    }
    .numberSearch{
        width:100%;
        display: inline-block;
        border:none;
        text-align: center;
        position: inherit;
        // margin:0 auto;
        margin-top: 15px;
        height: 53px;
        label{
            border-bottom: 3px double #812990;
            font-size: 26px;;
            line-height: 25px;
            padding-bottom: 0px;
            letter-spacing: -0px;;
            font-weight: normal;
            &:after{
                display: none;
            }
        }
    }

    .btnArea{
        text-align: center;
        width:100%;
        margin-top: 30px;;
        button{
            display:inline-block;
            background: #812990;
            height: auto;
            width:auto;
            color: #fff;
            font-size: 18px;
            padding: 7px 35px;
            text-align: left;
            margin:0 auto;
            
            i{ display: none ;}
        }
    }

    ul{
        margin-top: 40px;
        li{
            margin-left: 40px;;
            font-size: 14px;
            line-height: 1.25;;
            margin-bottom: 20px;;
            &::before{
                position: relative;
                left:-10px;
                content: " ";
                vertical-align:middle;
                margin-left: -5px;;
                width:4px;
                height: 4px;
                background-color: #812990;
                border-radius: 50%;
                display: inline-block;
            }
        }
    }
}



